<Window x:Class="ProjektTechniki.View.ActionLoadedBase"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjektTechniki.View"
        mc:Ignorable="d"
        Title="ActionLoadedBase" Height="600" Width="1000"
        DataContext="{Binding ActionLoadedBase,Source={StaticResource Locator}}">
    <Grid  >
        <DataGrid x:Name="LoadedData" ItemsSource="{Binding Table}" CanUserResizeColumns="False" IsReadOnly="True"
                 ColumnWidth="*" CanUserAddRows="False" AutoGenerateColumns="True"  />

    </Grid>
</Window>
